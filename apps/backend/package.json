{
  "name": "@ize/backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "scripts": {
    "build": "tsc --project ./",
    "start:dev": "nodemon src/express/server.ts",
    "start:dev:docker": "npx prisma db push && npm run start:dev",
    "start:prod": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/src/express/server.js",
    "cron": "CRON=true npx ts-node src/cron/cron.ts",
    "prisma-migrate-dev": "npx prisma migrate dev",
    "prisma-migrate-deploy": "npx prisma migrate deploy",
    "prisma-generate": "npx prisma generate",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org ize --project ize-backend ./dist && sentry-cli sourcemaps upload --org ize --project ize-backend ./dist",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/server": "^4.11.2",
    "@graphql-tools/load-files": "^7.0.0",
    "@graphql-tools/merge": "^9.0.10",
    "@graphql-tools/schema": "^10.0.9",
    "@hatsprotocol/sdk-v1-core": "^0.10.0",
    "@hatsprotocol/sdk-v1-subgraph": "^1.0.0",
    "@prisma/client": "^5.22.0",
    "@sentry/cli": "^2.39.0",
    "@sentry/node": "^8.40.0",
    "@sentry/profiling-node": "^8.40.0",
    "@telegram-auth/server": "^1.0.4",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/nanoid": "^3.0.0",
    "alchemy-sdk": "^3.5.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "deep-object-diff": "^1.1.9",
    "discord.js": "^14.16.3",
    "express": "^4.21.1",
    "graphql": "^16.9.0",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.8",
    "openai": "^4.73.0",
    "pg": "^8.13.1",
    "prisma": "^5.22.0",
    "reflect-metadata": "^0.2.2",
    "short-uuid": "^5.2.0",
    "stytch": "^11.10.0",
    "telegraf": "^4.16.3",
    "tldts": "^6.1.63",
    "viem": "^2.24.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@sentry/core": "^8.48.0",
    "@telegraf/types": "^7.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/node": "^22.9.2",
    "@types/validator": "^13.12.2",
    "nodemon": "^3.1.7",
    "sequelize-cli": "^6.6.2",
    "tsconfig-paths": "^4.2.0"
  }
}
